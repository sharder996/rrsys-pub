CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TABLES(TID INTEGER NOT NULL PRIMARY KEY,CAPACITY INTEGER)
CREATE MEMORY TABLE CUSTOMERS(CID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,FIRSTNAME VARCHAR(20),LASTNAME VARCHAR(20),PHONENUM VARCHAR(20))
CREATE MEMORY TABLE MENU(IID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(30),TYPE VARCHAR(20),DETAIL VARCHAR(100),PRICE DOUBLE)
CREATE MEMORY TABLE ORDERS(OID INTEGER NOT NULL PRIMARY KEY,PRICE DOUBLE,NOTE VARCHAR(100))
CREATE MEMORY TABLE RESERVATIONS(RID INTEGER NOT NULL PRIMARY KEY,CID INTEGER,TID INTEGER,NUMPEOPLE INTEGER,OID INTEGER,STARTTIME TIMESTAMP,ENDTIME TIMESTAMP,CONSTRAINT CONSTRAINT0 FOREIGN KEY(CID) REFERENCES CUSTOMERS(CID),CONSTRAINT CONSTRAINT1 FOREIGN KEY(TID) REFERENCES TABLES(TID))
ALTER TABLE CUSTOMERS ALTER COLUMN CID RESTART WITH 104
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
